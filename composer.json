{
    "name": "fdekker/symfony-log-viewer-bundle",
    "description": "A log viewer bundle for Symfony 6 and up",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "lock": false
    },
    "require": {
        "php": ">=8.1",
        "symfony/asset": " ^6.3||^7.0",
        "symfony/config": " ^6.3||^7.0",
        "symfony/dependency-injection": " ^6.3||^7.0",
        "symfony/finder": "^6.4",
        "symfony/framework-bundle": "^6.3||^7.0",
        "symfony/http-foundation": " ^6.3||^7.0",
        "symfony/http-kernel": " ^6.3||^7.0"
    },
    "require-dev": {
        "digitalrevolution/phpunit-file-coverage-inspection": "^v2.0.0",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.6",
        "phpmd/phpmd": "^2.14",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-symfony": "^1.3",
        "phpstan/extension-installer": "^1.3",
        "symfony/browser-kit": "^6.3||^7.0",
        "symfony/css-selector": "^6.3||^7.0",
        "symfony/http-client": "^6.3||^7.0",
        "symfony/messenger": "^6.3||^7.0",
        "symfony/monolog-bridge": "^6.3||^7.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/phpunit-bridge": "^6.3||^7.0",
        "symfony/templating": "^6.3||^7.0",
        "symfony/twig-bundle": "^6.3||^7.0",
        "symfony/uid": "^6.3||^7.0",
        "symfony/yaml": "^6.3||^7.0",
        "twig/twig": "^3.8.0"
    },
    "autoload": {
        "psr-4": {
            "FD\\SymfonyLogViewerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FD\\SymfonyLogViewerBundle\\Tests\\Functional\\": "tests/Functional/",
            "FD\\SymfonyLogViewerBundle\\Tests\\Unit\\": "tests/Unit/",
            "FD\\SymfonyLogViewerBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check": ["@check:phpstan", "@check:phpmd", "@check:phpcs"],
        "check:phpstan": "phpstan analyse",
        "check:phpmd": "phpmd src,tests text phpmd.xml.dist --suffixes php",
        "check:phpcs": "phpcs src tests",
        "fix": "@fix:phpcbf",
        "fix:phpcbf": "phpcbf src tests",
        "test": "phpunit",
        "test:integration": "phpunit --testsuite integration",
        "test:unit": "phpunit --testsuite unit"
    }
}
