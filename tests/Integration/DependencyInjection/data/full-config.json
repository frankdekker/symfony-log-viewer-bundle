{
    "fd_log_viewer": {
        "enable_default_monolog": true,
        "home_route": "home",
        "log_files": {
            "monolog": {
                "type": "monolog",
                "name": "TestMonolog",
                "finder": {
                    "in": "in-directory",
                    "name": "*.error.log",
                    "depth": ">= 1",
                    "ignoreUnreadableDirs": false,
                    "followLinks": true
                },
                "downloadable": true,
                "deletable": true,
                "start_of_line_pattern": "start-of-line",
                "log_message_pattern": "log-message-pattern",
                "date_format": "date-format"
            }
        }
    }
}
