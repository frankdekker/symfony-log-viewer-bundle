{
    "log_files": {
        "monolog": {
            "type": "monolog",
            "name": "TestMonolog",
            "downloadable": true,
            "deletable": true,
            "finder": {
                "in": "%kernel.logs_dir%",
                "name": "*.log",
                "depth": "== 0",
                "ignoreUnreadableDirs": true,
                "followLinks": false
            },
            "start_of_line_pattern": "/^\\[\\d{4}-\\d{2}-\\d{2}[^]]*]\\s+\\S+\\.\\S+:/",
            "log_message_pattern": "/^\\[(?P<date>[^\\]]+)\\]\\s+(?P<channel>[^\\.]+)\\.(?P<severity>[^:]+):\\s+(?P<message>.*)\\s+(?P<context>(?:{.*?}|\\[.*?]))\\s+(?P<extra>(?:{.*?}|\\[.*?]))\\s+$/s",
            "date_format": null
        }
    },
    "hosts": {
        "localhost": {
            "name": "Local",
            "host": null
        }
    }
}
